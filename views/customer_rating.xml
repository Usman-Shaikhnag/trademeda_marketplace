    <!-- Form view for customer.testimony -->
    <record id="customer_testimony_form" model="ir.ui.view">
        <field name="name">customer.testimony.form</field>
        <field name="model">customer.testimony</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="partner_id"/>
                    <field name="rating"/>
                    <field name="testimony"/>
                    <field name="display"/>

                </group>
            </form>
        </field>
    </record>

    <!-- Tree view for customer.testimony -->
    <record id="customer_testimony_tree" model="ir.ui.view">
        <field name="name">customer.testimony.tree</field>
        <field name="model">customer.testimony</field>
        <field name="arch" type="xml">
            <tree>
                <field name="partner_id"/>
                <field name="rating"/>
                <field name="testimony"/>
                <field name="display"/>

                
            </tree>
        </field>
    </record>
