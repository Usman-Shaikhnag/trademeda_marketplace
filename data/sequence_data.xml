<odoo>
    <data noupdate="1">
        <!-- Define a sequence -->
        <record id="rfq_sequence" model="ir.sequence">
            <field name="name">RFQ Sequence</field>
            <field name="code">trademeda.rfq</field>
            <field name="prefix">RFQ</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
        </record>

        <record id="rfq_quotation_sequence" model="ir.sequence">
            <field name="name">Quotation Sequence</field>
            <field name="code">trademeda.rfq_quotation</field>
            <field name="prefix">QUOTATION</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>